<h3>Index</h3>

<p>{{link-to 'New stat_line' 'stat_lines.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>Player</th>
    <th>Game</th>
    <th>Ab</th>
    <th>1b</th>
    <th>2b</th>
    <th>3b</th>
    <th>Hr</th>
    <th>Bb</th>
    <th>Hbp</th>
    <th>K</th>
    <th>Sac</th>
    <th>Rbi</th>
    <th>Sb</th>
  </tr>
  {{#each}}
    <tr>
      <td>{{link-to id 'stat_lines.show' this}}</td>
      <td>{{player_id}}</td>
      <td>{{game_id}}</td>
      <td>{{ab}}</td>
      <td>{{1b}}</td>
      <td>{{2b}}</td>
      <td>{{3b}}</td>
      <td>{{hr}}</td>
      <td>{{bb}}</td>
      <td>{{hbp}}</td>
      <td>{{k}}</td>
      <td>{{sac}}</td>
      <td>{{rbi}}</td>
      <td>{{sb}}</td>
    </tr>
  {{/each}}
</table>
